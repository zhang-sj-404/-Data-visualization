<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.czu.zsj.mapper.MasterMapper">
    <select id="findAdminUserByPhone" resultType="com.czu.zsj.pojo.Master">
        SELECT * from master WHERE m_phone=#{m_phone} AND m_password=#{m_pass}
    </select>
</mapper>